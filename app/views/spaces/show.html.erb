<%= render template: "spaces/_show" %>

<div class="right-container">
  <h2>Reserve this Spot</h2>
  <%= render template: "reservations/_form", locals: { space: @space, reservation: @reservation } %>

  <h4>Your Reservations for this Spot: </h4>
  <% @users_reservations.each do |reservation| %>
    <%= render template: "reservations/_show", locals: {reservation: reservation} %>
  <% end %>
</div>
